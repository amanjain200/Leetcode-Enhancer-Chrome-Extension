#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules/ts-node/dist/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules/ts-node/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules/ts-node/dist/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules/ts-node/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.9.3_@types+node@22.7.4_typescript@5.5.4/node_modules:/mnt/c/Users/jains/OneDrive/Desktop/Browser_Extension/chrome-extension-boilerplate-react-vite-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-script-deprecated.js" "$@"
fi
